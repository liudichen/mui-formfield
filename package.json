{
  "name": "mui-formfield",
  "version": "0.3.1",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": {
    "name": "liudichen",
    "email": "liudichen@foxmail.com",
    "url": "https://www.iimm.ink"
  },
  "description": "mui form field components with simple wrapper, ready for formily",
  "repository": {
    "url": "https://github.com/liudichen/mui-formfield",
    "type": "https"
  },
  "keywords": [
    "mui",
    "material-ui",
    "form",
    "form field",
    "fieldWrapper",
    "iimm",
    "www.iimm.ink",
    "liudichen",
    "Upload",
    "react",
    "@mui/material",
    "component",
    "react"
  ],
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "es",
    "lib"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^11.0.1",
    "eslint-plugin-react": "^7.29.4",
    "father-build": "^1.17.2",
    "gh-pages": "^3.2.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "typescript": "^4.6.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-react": "^5.0.2",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@formily/core": "^2.1.11",
    "@formily/react": "^2.1.11",
    "@formily/reactive": "^2.1.11",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "5.0.0-alpha.74",
    "@mui/material": "^5.9.2",
    "@mui/styles": "^5.9.2",
    "@mui/system": "^5.9.2",
    "@mui/x-data-grid": "^5.15.0",
    "@tabler/icons": "^1.78.1",
