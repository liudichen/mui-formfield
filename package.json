    "ckeditor5-custom-build": "file:ckeditor5-custom-build",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.4",
    "mui-component": "^0.1.42",
    "mui-image-crop": "^0.1.6",
    "react": ">=17.0.0",
    "react-device-detect": "^2.2.2",
    "react-dom": ">=17.0.0",
    "react-dropzone": "^12.1.0",
    "react-responsive-carousel": "^3.2.23",
    "xlsx": "^0.18.5"
  }
