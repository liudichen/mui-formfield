{
  "private": true,
  "name": "mui-formfield",
  "version": "0.1.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "author": {
    "name": "liudichen",
    "email": "liudichen@foxmail.com",
    "url": "https://www.iimm.ink"
  },
  "description": "mui form field components with simple wrapper, ready for formily",
    "repository": {
    "url": "https://github.com/liudichen/mui-formfield",
    "type": "https"
  },
    "keywords": [
    "mui",
    "material-ui",
    "form",
    "form field",
    "fieldWrapper",
    "iimm",
    "www.iimm.ink",
    "liudichen",
    "Upload",
    "react",
    "@mui/material",
    "layout",
    "react"
  ],
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": ["es","lib"],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "@mui/material": ">=5.0.0",
    "react":">=16.9.0",
    "react-dom":">=16.9.0"
